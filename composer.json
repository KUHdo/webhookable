{
    "name": "kuhdo/webhookable",
    "type": "library",
    "description": "Webhook laravel models easily",
    "keywords": ["laravel", "models","post","hooks","webhook"],
    "homepage": "https://www.kuhdo.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Arne Bartelt",
            "email": "Arne.Bartelt@kuhdo.de",
            "homepage": "http://www.kuhdo.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "^5.8",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/notifications": "^5.8"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "orchestra/database": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "KUHdo\\Webhookable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KUHdo\\Webhookable\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KUHdo\\Webhookable\\WebhookableProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
